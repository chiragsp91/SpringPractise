import org.springframework.web.bind.annotation.RestController;

@RestController
public class BookController {
	@GetMapping("/books")
	public List<Book> getAllBooks(){
		return Arrays.asList(
				new Book(0, null, null));
		List<Book> bookList = new ArrayList<Book>();
	}
	
}
